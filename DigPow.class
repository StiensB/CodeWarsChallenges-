public class DigPow {

 public static long digPow(int n, int p) {
   // your code
   long temp = 0;
   String[] arr = String.valueOf(n).split("");
   for (int i = 0; i < arr.length; i++) {
     temp += Math.pow(Double.parseDouble(arr[i]), (p + i));
   }
   return temp % n == 0 ? temp /n : -1;
 }

}
